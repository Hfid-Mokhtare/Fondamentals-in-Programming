//Q3 :

#include <iostream>
using namespace std;


int main()
{
	bool Result;

	//

    Result = (7 == 7) && (7 > 5);
    cout << Result << endl;
    
    Result = (7 == 7) && (7 < 5);
    cout << Result << endl;
    
    Result = (7 == 7) || (7 < 5);
    cout << Result << endl;
    
    Result = (7 < 7) || (7 > 5);
    cout << Result << endl;
    
    Result = !(7 == 7) && (7 > 5);
    cout << Result << endl;
    
    Result = (7 == 7) && !(7 < 5);
    cout << Result << endl;


	return 0;
}

===============================================================================

//Q4 :

#include <iostream>
using namespace std;


int main()
{
	bool Result;

	//

    Result = (5 > 6 && 7 == 7) || (1 || 0);
    cout << Result << endl;
    
    Result = !(5 > 6 && 7 == 7) || (1 || 0);
    cout << Result << endl;
    
    Result = !(5 > 6 && 7 == 7) || !(1 || 0);
    cout << Result << endl;
    
    Result = !(5 > 6 || 7 == 7) && !(1 || 0);
    cout << Result << endl;
    
    Result = ((5 > 6 && 7 <= 8) || (8 > 1 && 4 <= 3)) && 1;
    cout << Result << endl;
    
    Result = ((5 > 6 && !(7 <= 8)) && (8 > 1 || 4 <= 3)) || 1;
    cout << Result << endl;


	return 0;
}
