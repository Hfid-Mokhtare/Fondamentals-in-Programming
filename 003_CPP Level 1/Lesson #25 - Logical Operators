//Q2 : 

#include <iostream>
using namespace std;


int main()
{
	bool Result;

	//

	Result = (1 && 1);
	cout << Result << endl;

	Result = (true && 0);
	cout << Result << endl;

	Result = (0 || 1);
	cout << Result << endl;

	Result = (0 || 0);
	cout << Result << endl;

	Result = (!0);
	cout << Result << endl;

	Result = !(1 || 0);
	cout << Result << endl;


	return 0;
}

