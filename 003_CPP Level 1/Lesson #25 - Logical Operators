// Q1 : 

#include <iostream>
using namespace std;


int main()
{
	bool Result;

	//

	Result = (12 >= 12);
	cout << Result << endl;

	Result = (12 > 7);
	cout << Result << endl;

	Result = (8 < 6);
	cout << Result << endl;

	Result = (8 == 8);
	cout << Result << endl;

	Result = (12 <= 12);
	cout << Result << endl;

	Result = (7 == 5);
	cout << Result << endl;

	Result = !(12 >= 12);
	cout << Result << endl;

	Result = !(12 > 7);
	cout << Result << endl;

	Result = !(8 < 6);
	cout << Result << endl; 

	Result = !(8 == 8);
	cout << Result << endl;

	Result = !(12 <= 12);
	cout << Result << endl;

	Result = !(7 == 5);
	cout << Result << endl;

	return 0;
}

